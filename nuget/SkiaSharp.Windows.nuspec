    <file src="output/docs/msxml/SkiaSharp.xml" target="lib/net45" />
    <file src="output/uwp/SkiaSharp.dll" target="lib/uap10.0" />
    <file src="output/docs/msxml/SkiaSharp.xml" target="lib/uap10.0" />
